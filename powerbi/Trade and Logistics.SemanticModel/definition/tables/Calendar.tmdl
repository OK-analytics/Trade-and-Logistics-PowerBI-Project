	column 'Month (groups)' =
			SWITCH(
				TRUE,
				ISBLANK('Calendar'[Month]),
				"(Blank)",
				'Calendar'[Month] IN {"September 2016",
					"October 2016",
					"November 2016",
					"December 2016"},
				"2016",
				'Calendar'[Month] IN {"January 2017",
					"February 2017",
					"March 2017",
					"April 2017",
					"May 2017",
					"June 2017",
					"July 2017",
